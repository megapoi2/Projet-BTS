<Window x:Class="Raminagrobis.WPF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Raminagrobis.WPF"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="1202.8">
    <Grid>
        <StackPanel Orientation="Horizontal" Height="35" VerticalAlignment="Top">
            <Button Content="Page Adherents" MinWidth="100" Margin="10 0 0 0" Click="BtnAdherent"/>
            <Button Content="Page Fournisseur" MinWidth="100" Margin="10 0 0 0" Click="BtnFournisseur"/>
            <Button Content="Page Paniers" MinWidth="100" Margin="10 0 0 0" Click="BtnPaniers"/>
            <Button Content="Page Produits" MinWidth="100" Margin="10 0 0 0" Click="BtnProduits"/>
            <Button Content="Page Insert" MinWidth="100" Margin="250 0 0 0" Click="BtnInsert"/>
            <Button Content="Page Update" MinWidth="100" Margin="10 0 0 0" Click="BtnUpdate"/>
            <Button Content="Page Delete" MinWidth="100" Margin="10 0 0 0" Click="BtnDelete"/>
        </StackPanel>
        <Frame x:Name="Main" NavigationUIVisibility="Hidden" Margin="0 35 0 0" />
    </Grid>
</Window>